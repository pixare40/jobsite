<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  
    <system.serviceModel>
      <services>
        <service name="JobMtaani.Business.Managers.AdManager">
          <endpoint address="net.tcp://localhost:8009/AdService"
                    binding="netTcpBinding"
                    contract="JobMtaani.Business.Contracts.IAdService" />
        </service>
        <service name="JobMtaani.Business.Managers.AccountManager">
          <endpoint address="net.tcp://localhost:8009/AccountService"
                    binding="netTcpBinding"
                    contract="JobMtaani.Business.Contracts.IAccountService" />
        </service>
      </services>
      <bindings>
        <netTcpBinding>
          <binding transactionFlow="true" maxReceivedMessageSize="2000000">
            <reliableSession enabled="true"/>
          </binding>
        </netTcpBinding>
      </bindings>
    </system.serviceModel>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
</configuration>